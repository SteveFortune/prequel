{
  "name": "prequel",
  "version": "1.0.0-pre.0",
  "description": "Query JavaScript objects with SQL",
  "main": "index.js",
  "scripts": {
    "build-parser": "pegjs src/sql.pegjs build/parser.js",
    "pretest": "npm run build-parser",
    "test": "nyc tape \"./test/**/*.js\" | faucet && nyc report --reporter text",
    "coveralls": "nyc report --reporter text-lcov | coveralls",
    "clean": "rm -rf build/* coverage"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "config": {
    "nyc": {
      "require": [
        "babel-register",
        "babel-polyfill"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwhitfieldseed/prequel.git"
  },
  "keywords": [
    "object",
    "sql",
    "query"
  ],
  "author": "Joe Whitfield-Seed <joeseed86@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwhitfieldseed/prequel/issues"
  },
  "homepage": "https://github.com/jwhitfieldseed/prequel#readme",
  "dependencies": {
    "lodash.sortbyorder": "^3.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-register": "^6.2.0",
    "coveralls": "^2.11.4",
    "faucet": "0.0.1",
    "istanbul": "^0.3.17",
    "lodash": "^3.10.0",
    "nyc": "^4.0.0",
    "pegjs": "^0.8.0",
    "sql.js": "^0.2.21",
    "tape": "^4.0.1"
  }
}
