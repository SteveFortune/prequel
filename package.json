{
  "name": "prequel",
  "version": "1.0.0-pre.0",
  "description": "Query JavaScript objects with SQL",
  "main": "index.js",
  "scripts": {
    "build-parser": "pegjs src/sql.pegjs build/parser.js",
    "pretest": "npm run build-parser",
    "test": "babel-node $(which tape) \"./test/**/*.js\" | faucet",
    "coverage": "istanbul cover babel-node ./test/e2e-tests.js",
    "clean": "rm build/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwhitfieldseed/prequel.git"
  },
  "keywords": [
    "object",
    "sql",
    "query"
  ],
  "author": "Joe Whitfield-Seed <joeseed86@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwhitfieldseed/prequel/issues"
  },
  "homepage": "https://github.com/jwhitfieldseed/prequel#readme",
  "dependencies": {
    "lodash.sortbyorder": "^3.4.4"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "faucet": "0.0.1",
    "istanbul": "^0.3.17",
    "pegjs": "^0.8.0",
    "sql.js": "^0.2.21",
    "tape": "^4.0.1"
  }
}
